{
  "name": "@dxos/codec-protobuf",
  "version": "1.0.0",
  "description": "Value encoding for protobuf.",
  "homepage": "https://github.com/dxos/codec-protobuf#readme",
  "bugs": {
    "url": "https://github.com/dxos/codec-protobuf/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dxos/codec-protobuf.git"
  },
  "license": "GPL-3.0",
  "main": "index.js",
  "browser": "dist/codec-protobuf.js",
  "files": [
    "dist",
    "index.js"
  ],
  "browserslist": [
    "> 5%"
  ],
  "scripts": {
    "lint": "eslint 'src/**/*.js'",
    "build": "npm run clean && npm run build:protobuf && npm run build:babel",
    "build:babel": "babel ./src --out-dir ./dist --ignore \"**/*.test.js\" --source-maps",
    "build:protobuf": "pbjs -t json src/schema.proto -o src/schema.json && pbjs -t json src/schema-test-one.proto -o src/schema-test-one.json",
    "clean": "del-cli dist",
    "test": "npm run build:protobuf && jest --verbose --passWithNoTests src",
    "posttest": "npm run lint",
    "prepublishOnly": "npm run test && npm run build"
  },
  "dependencies": {
    "protobufjs": "https://github.com/wirelineio/protobuf.js#wireline-dist",
    "source-map-support": "^0.5.12"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/preset-env": "^7.4.5",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-inline-import": "^3.0.0",
    "del-cli": "^2.0.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint": "^5.16.0",
    "jest": "^24.5.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
